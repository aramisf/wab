{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "setdb": "bash bin/setdb.sh -d",
    "dropdb": "bash bin/setdb.sh -D",
    "set-db-user": "bash bin/setdb.sh -u",
    "drop-db-user": "bash bin/setdb.sh -U",
    "db-up": "yarn run set-db-user && yarn run setdb && yarn run db-seed",
    "db-down": "yarn run dropdb && yarn run drop-db-user",
    "db-seed": "bash bin/setdb.sh -s",
    "start": "nodemon bin/www",
    "dev": "yarn db-down && yarn db-up && yarn start"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.11.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
